{
  "cli": {
    "version": ">= 7.0.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "channel": "production",
      "android": {
        "image": "sdk-52",
        "buildType": "app-bundle"
      },
      "env": {
        "EXPO_PUBLIC_OPENAI_API_KEY": "SECRETS"
      }
    }
  },
  "submit": {
    "production": {}
  }
}